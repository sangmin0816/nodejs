<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <%- include('../head.ejs') %>
    <title>Sample</title>
</head>
<body>
    <%-include('../header.ejs') %>

    <div class="container">
        <h1 class="title">Sample List</h1>
        <table class="table">
            <thead>
                <tr>
                    <td>no</td>
                    <td>name</td>
                </tr>
            </thead>
            <tbody>
            <% for(var i=0; i<samples.length; i++){%>
            <tr>
                <td><%=samples[i].no%></td>
                <td><a href="/sample/get/<%=samples[i].no%>"><%=samples[i].NAME%></a></td>
            </tr>
            <%}%>
            </tbody>
        </table>
        <a href="/sample/add" class="button is-primary">Add</a>
    </div>
    
</body>
</html>